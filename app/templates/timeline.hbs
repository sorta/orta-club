{{#if @model.loading}}
  Loading...
{{else}}
  {{#each @model as |year|}}
    <h2>
      {{year.num}}
      {{#each year.donnings as |donning|}}
        <div>
          {{donning.member.nameFirst}}
          wore the
          {{donning.gayApparel.name}}
          in
          {{donning.location.name}}
        </div>
      {{else}}
        <div>This information is not yet known!</div>
      {{/each}}
    </h2>
    <br/>
    <br/>
  {{/each}}
{{/if}}

{{outlet}}